layer {
  name: "concat_headpose_eyeappearance"
  type: "Concat"
  bottom: "ip1"
  bottom: "headpose"
  top: "cat"
}

layer {
  name: "ip2"
  type: "InnerProduct"
  bottom: "cat"
  top: "ip2"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  inner_product_param {
    num_output: 2
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "accuracy"
  type: "AccuracyGaze"
  bottom: "ip2"
  bottom: "gaze"
  top: "accuracy"
  include: { phase: TEST }
}

layer {
  name: "loss"
  type: "EuclideanGazeLoss"
  bottom: "ip2"
  bottom: "gaze"
  top: "loss"
}